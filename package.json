{
  "name": "jukebox-duo-websocket-server",
  "version": "1.0.0",
  "description": "TypeScript WebSocket server for JukeboxDuo app",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "socket.io": "^4.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}